<!-- 2. menu samping -->
<div id="wrapper">
    <div id="sidebar-wrapper">
        <ul class="sidebar-nav section">
            <li><ul class="collapsible collapsible-accordion repair">
                <li>
                    <a class="collapsible-header black repair">ANTRIAN</a>
